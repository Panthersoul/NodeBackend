<div class="d-flex " id="contenedor">
    <div>
        <h1>PRODUCTOS</h1><br>
        {{#if data.length}}
        <table class="table table-success table-striped">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Title</th>
                    <th scope="col">Price</th>
                    <th scope="col">Thumbnail</th>
                </tr>
            </thead>
            <tbody>
                {{#each data}}
                    <tr>
                        <td>{{this.id}}</td>
                        <td>{{this.title}}</td>
                        <td>${{this.price}}</td>
                        <td><img style="width: 50px; height:50px" src={{this.thumbnail}} alt=""></td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
        {{else}}
            <p>NO HAY DATOS CARGADOS</p>
        {{/if}}
    </div>
</div>
